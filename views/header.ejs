<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <% if ( typeof title == 'undefined' ) { %>
    <title>BeRoyaler</title>
  <% } else { %>
    <title><%- title %></title>
  <% } %>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <% if ( typeof og_image != 'undefined') { %>
    <meta property="og:image" content="<%- og_image %>" />
    <meta property="og:image:width" content="300" />
    <meta property="og:image:height" content="300" />
  <% } %>
  <link href='https://fonts.googleapis.com/css?family=Roboto:400,300,900,100' rel='stylesheet' type='text/css'>
  <script src="/assets/styles.bundle.js"></script>
  <% if (ENV_DEVELOPMENT) { %>
  <% } %>
</head>
<body>
  <div class="container">
    <header class="siteHeader">
      <a href="/#/"><img src="/static/images/BeRoyal.png" /></a>
    </header>
